{# templates/display_users/index.html.twig #}
{% extends 'base.html.twig' %}

{% block title %}Registered Users{% endblock %}

{% block content %}
    <div class="">
        <h2>Registered Users</h2>
        <table class="table">
            <thead>
                <tr>
                    <th>ID</th>
                    <th>Email</th>
                    <!-- Add other fields as needed -->
                </tr>
            </thead>
            <tbody>
                {% for user in users %}
                    <tr>
                        <td>{{ user.id }}</td>
                        <td>{{ user.email }}</td>
                        <!-- Display other fields as needed -->
                    </tr>
                {% endfor %}
            </tbody>
        </table>
    </div>
{% endblock %}
